# Environment variables for development
VITE_APP_NAME=Refugee Health Assistant
VITE_APP_VERSION=0.1.0

# Firebase Configuration (Development)
VITE_FIREBASE_API_KEY=your_dev_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Map Configuration
VITE_DEFAULT_MAP_CENTER_LAT=39.9334
VITE_DEFAULT_MAP_CENTER_LNG=32.8597
VITE_DEFAULT_MAP_ZOOM=10

# Voice API Configuration
VITE_VOICE_API_ENABLED=true
VITE_SPEECH_RECOGNITION_LANG=en-US

# Feature Flags
VITE_ENABLE_OFFLINE_MODE=true
VITE_ENABLE_VOICE_INPUT=true
VITE_ENABLE_SAFE_ROUTE=true
VITE_ENABLE_PWA=true

# API Endpoints (if using external services)
VITE_ELEVENLABS_API_KEY=your_elevenlabs_key_here
VITE_TRANSLATION_API_KEY=your_translation_key_here

# Debug flags
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug